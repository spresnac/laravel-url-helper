{
  "name": "spresnac/laravel-url-helper",
  "description": "Helping you with URL related problems",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "php",
    "laravel"
  ],
  "authors": [
    {
      "name": "Sascha Presnac",
      "email": "presnac@gmail.com",
      "homepage": "https://twitter.com/spresnac"
    }
  ],
  "autoload": {
    "psr-4": {
