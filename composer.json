    }
  },
  "autoload-dev": {
    "psr-4": {
      "spresnac\\tests\\": "tests/spresnac/"
    }
  },
  "funding": [
    {
      "type": "other",
      "url": "https://www.buymeacoffee.com/spresnac"
    },
    {
      "type": "paypal",
      "url": "https://www.paypal.me/paladin"
    },
    {
      "type": "github",
      "url": "https://github.com/spresnac"
    }
  ],
  "require": {
    "php": "^8",
    "ext-simplexml": "*",
    "ext-curl": "*"
